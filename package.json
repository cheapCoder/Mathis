{
	"name": "mathis",
	"publisher": "happyHacking",
	"displayName": "mathis",
	"description": "show what match your configure in your project",
	"version": "0.1.1",
	"repository": "https://github.com/necessityOVO/mathis",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/locale/**/*.{js,ts,jsx,tsx,json}"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [],
		"languages": [
			{
				"id": "svelte",
				"aliases": [
					"Svelte",
					"svelte"
				],
				"extensions": [
					".svelte"
				]
			}
		],
		"keybindings": [
			{
				"key": "cmd+shift+v",
				"command": "mathis.searchFromClipboard"
			}
		],
		"configuration": [
			{
				"title": "mathis",
				"properties": {
					"defSelect": {
						"title": "defSelect",
						"description": "select the key or the value part",
						"type": "array"
					},
					"applyRange": {
						"type": "string",
						"description": "def文件显示应用的代码块显示的范围"
					},
					"pathSlice": {
						"type": "boolean",
						"default": "true",
						"description": "显示路径时去除src之前的部分"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/acorn": "^4.0.6",
		"@types/glob": "^7.2.0",
		"@types/json-to-ast": "^2.1.2",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@babel/types": "^7.16.0",
		"acorn": "^8.7.0",
		"json-to-ast": "^2.1.0"
	}
}

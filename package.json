{
	"name": "mathis",
	"publisher": "happyHacking",
	"displayName": "Mathis",
	"description": "一个辅助国际化显示的轻量插件",
	"version": "0.2.2",
	"repository": "https://github.com/necessityOVO/mathis",
	"icon": "img/logo.png",
	"main": "./out/src/extension.js",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/locale/**/*.{js,ts,jsx,tsx,json}"
	],
	"contributes": {
		"commands": [],
		"languages": [
			{
				"id": "svelte",
				"aliases": [
					"Svelte",
					"svelte"
				],
				"extensions": [
					".svelte"
				]
			}
		],
		"keybindings": [
			{
				"key": "cmd+shift+v",
				"command": "mathis.searchFromClipboard"
			}
		],
		"configuration": [
			{
				"title": "Mathis",
				"properties": {
					"mathis.lazyLoadApply": {
						"order": 0,
						"description": "是否直到进入国际化定义文件才加载字段应用列表",
						"type": "boolean",
						"default": true
					},
					"mathis.detectApplyWay": {
						"order": 1,
						"description": "检测应用节点的方式\nreg:正则匹配; split:分词在def中查找(检测更完备)",
						"type": "string",
						"default": "split",
						"enum": [
							"reg",
							"split"
						]
					},
					"mathis.defSelect": {
						"order": 2,
						"description": "跳转定义文件时选择字段的哪些部分",
						"type": "string",
						"default": "value",
						"enum": [
							"key",
							"value",
							"key and value"
						]
					},
					"mathis.pathSlice": {
						"order": 3,
						"type": "boolean",
						"default": true,
						"description": "显示路径时去除src之前的部分"
					}
				}
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/acorn": "^4.0.6",
		"@types/glob": "^7.2.0",
		"@types/json-to-ast": "^2.1.2",
		"@types/lodash": "^4.14.179",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@types/vscode": "^1.62.0",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"@vscode/test-electron": "^1.6.2",
		"eslint": "^8.1.0",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4"
	},
	"dependencies": {
		"@babel/types": "^7.16.0",
		"acorn": "^8.7.0",
		"json-to-ast": "^2.1.0",
		"lodash": "^4.17.21"
	}
}
